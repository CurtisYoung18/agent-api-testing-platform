{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "installCommand": "npm install && npx prisma generate",
  "rewrites": [
    {
      "source": "/api/agents/:id",
      "destination": "/api/agent-detail"
    },
    {
      "source": "/api/history/:id",
      "destination": "/api/history"
    },
    {
      "source": "/api/download",
      "destination": "/api/download"
    },
    {
      "source": "/api/tests",
      "destination": "/api/tests"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "includeFiles": "node_modules/.prisma/client/**",
      "maxDuration": 60
    }
  }
}
